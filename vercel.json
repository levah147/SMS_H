{
    "version":2,
    "builds":[{
        "src": "student_management_system/wsgi.py",
        "use":"@vercel/python",
        "config":{ "maxLambdaSize": "15mb", "runtime": "Python 3.10.6"}
    },
    
    {"src": "build.sh",
    "use": "@vercel/static-build",
    "config":{ "disDir": "staticfiles_build"
}   
}],
    "routes":[
        {
            "src": "static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "student_management_system/wsgi.py"
        }
    ]
}